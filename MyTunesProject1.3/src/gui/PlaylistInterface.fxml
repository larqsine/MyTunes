<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PlaylistInterfaceController">
   <children>
      <Label layoutX="53.0" layoutY="14.0" text="Name:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="nameOfPlaylist" focusTraversable="false" layoutX="136.0" layoutY="16.0" />
      <Button focusTraversable="false" layoutX="474.0" layoutY="296.0" mnemonicParsing="false" onAction="#savePlaylist" text="SavePlaylist" />
      <Button focusTraversable="false" layoutX="474.0" layoutY="338.0" mnemonicParsing="false" onAction="#editPlaylist" text="EditPlaylist" />
      <Label layoutX="34.0" layoutY="51.0" text="Amount:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="amountOfPlaylistSongs" focusTraversable="false" layoutX="136.0" layoutY="52.0" />
      <Label layoutX="63.0" layoutY="89.0" text="Time:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <TextField fx:id="timeOfPlaylist" focusTraversable="false" layoutX="136.0" layoutY="90.0" />
   </children>
</AnchorPane>
